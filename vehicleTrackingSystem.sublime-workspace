ab2', data);\n        //for(dat in data){\n            // demo.innerHTML = data.message;\n            console.log(data.message);\n        //}\n\n        socket.emit('client','Hello from client');\n\n    });\n\n\n\n\nfunction queryActivities(loc) {\n    // console.log('Data sent!!');\n    socket.emit('location', loc);\n}\n\n\n\n// ......\n\n\nvar location_list = ['Near Arm Futsal', 'Dilli Bazar', 'Anamnagar', 'Putalisadak'];\nvar issuedDate = ['2015-11-12', '2015-11-16', '2015-11-18', '2015-11-20'];\n// var status = ['done', 'done', 'pending', 'pending'];\n\n\n\n\n\nvar parsedData = [['Near Arm Futsal', '2015-11-12', 'done'], ['Dilli Bazar', '2015-11-16', 'done'], \n    ['Putalisadak', '2015-11-20', 'pending'], ['Anamnagar', '2015-11-22', 'pending']]; \n\n\n\nvar activity_list = ['Collect cash from futsal Kirana', 'Deposit 10 gallons of water', \n'Deposit 10 cartoons of chow chow', 'Meet Ram Prasad and talk about cash', 'List note of new orders '];\nvar stat = [0, 0, 1, 1, 1];\n\n\n\n\n\nvar actTable = document.getElementById('vehicle_activities_table');\nfor(i = 0; i < parsedData.length; i++){\n    var statStr; \n    if(stat[i] == 0){\n        statStr = 'Done';\n    }\n    else {\n        statStr = 'Pending'; \n    }\n    var newTableRow = document.createElement('tr');\n    newTableRow.setAttribute('id', 'tr' + (i+1));\n    newTableRow.classList.add(\"tableRows\");\n    for (var j = 0; j < 3; j++){\n        var newTableCol = document.createElement('td');\n        newTableCol.innerHTML = data;\n        newTableCol.setAttribute('id', 'td' + (i+1) + (j+1));\n        newTableCol.setAttribute('align', 'center');\n        newTableCol.classList.add('tableCols');\n        newTableRow.appendChild(newTableCol);\n    }\n    // newTableRow.innerHTML = '<td>' + location_list[i] + '</td><td>' + issuedDate[i] + '</td><td>' + statStr + '</td>'; \n    actTable.appendChild(newTableRow);\n}\nvar lent = document.getElementsByClassName('tableCols');\n\n\nfor(i = 0; i < lent.length; i++)